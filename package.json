{
    "name": "fhir-mapping-language-support",
    "displayName": "FHIR Mapping Language Support",
    "description": "Support for the FHIR mapping language",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "map",
            "aliases": ["FHIR Mapping Language", "map"],
            "extensions": [".map"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "map",
            "scopeName": "source.map",
            "path": "./syntaxes/map.tmLanguage.json"
        }]
    }
}